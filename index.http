GRAPHQL http://localhost:8080/graphql

query user {
    userPage(query: {username: "", pageInfo: {page: 1, size: 10, sort: ["id"]}}) {
        content {
            ... on User {
                id,
                username,
                email,
                admin,
                loginTime,
                createTime,
                updateTime
            }
        }
        totalElement,
        __typename
    }
    userById(id: 1) {
        id,
        username,
        email,
        admin,
        loginTime,
        createTime,
        updateTime
    }
}

{
  "page": 1,
  "size": 10
}

